#!/usr/bin/make -f

.PHONY: override_dh_strip override_dh_auto_install override_dh_installdocs override_dh_installchangelogs

override_dh_strip:
	dh_strip --dbg-package=vdr-plugin-restfulapi-dbg

override_dh_auto_install:
	LIBDIR=. dh_auto_install --destdir=debian/vdr-plugin-restfulapi
	rm -rf debian/vdr-plugin-restfulapi/etc/vdr/plugins/restfulapi

override_dh_installdocs:

override_dh_installchangelogs:

%:
	dh $@ --parallel
